<UserControl x:Class="PanzerGeneral2_0.Controls.Other.Explosion"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"             
             xmlns:gif="http://wpfanimatedgif.codeplex.com"
             xmlns:local="clr-namespace:PanzerGeneral2_0.Controls.Other"             
             gif:ImageBehavior.AnimateInDesignMode="True"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Image gif:ImageBehavior.AnimatedSource="/PanzerGeneral2_0;component/Resources/Explosion/explosion.gif" 
               gif:ImageBehavior.RepeatBehavior="1x" />
    </Grid>
</UserControl>
